var adstat_obj=document.getElementById("adstat_js");if(!(/bot|spider/i.test(window.navigator.userAgent))&&adstat_obj&&adstat_obj.src){var adstat_domain="",adstat_cookie_domain="",adlocation_domain="",adstat_write=null,adstat_special=null,adstat_cookie=null,adstat_temp=[],adstat_cookie_main="z_day",exp_time=0,adstat_cookie_name=adstat_cookie_value="";var adstat_a=document.createElement("a");adstat_a.href=adstat_obj.src;adstat_domain=adstat_a.hostname;if("tj.pchome.net"==adstat_domain){adstat_cookie_domain=".pchome.net"}else{adstat_cookie_domain=adstat_domain.replace("stat","")}adlocation_domain=document.location.hostname;if(adstat_domain){adstat_write=function(a,b){if(!a){return}if(undefined==b){b="js"}switch(b){case"iframe":document.write('<iframe width=1 height=0 src="'+a+'" allowtransparency align="center,center" marginwidth=0 marginheight=0 scrolling=no frameborder=0></iframe>');break;case"js":document.write('<script type="text/javascript" src="'+a+'"><\/script>');break;default:}};adstat_special={".zol.com.cn":{"detail.zol.com.cn":9}};adstat_temp=adstat_special[adstat_cookie_domain];if(undefined!=adstat_temp&&undefined!=adstat_temp[adlocation_domain]){adstat_cookie={get:function(e){var a="",d=" "+document.cookie+";",b=d.indexOf((" "+e+"="));if(b>=0){b+=e.length+2;a=unescape(d.substring(b,d.indexOf(";",b)))}return a},set:function(e,c){var b=arguments,d=b.length;document.cookie=e+"="+c+((d>2&&b[2]!="")?";expires="+(typeof(b[2])=="object"?b[2].toGMTString():(new Date(b[2]*1000)).toGMTString()):"")+";path="+((d>3&&b[3]!="")?b[3]:"/")+";domain="+((d>4&&b[4]!="")?b[4]:".zol.com.cn")},checksub:function(e,d){var a=e.split("&"),b=a.length,f;if(b==1){return e.indexOf(d)}else{for(var c=0;c<b;c++){f=a[c].split("=");if(f[0]==d){return c}}}return -1},getsub:function(d,a){var c=this.get(d);var b=this.checksub(c,a);if(b>-1){return c.split("&")[b].split("=")[1]}else{if(c.indexOf(a)>0){return c.split("=")[1]}}return""},setsub:function(i,e,d){var h=this.get(i),c=arguments,g=c.lengt